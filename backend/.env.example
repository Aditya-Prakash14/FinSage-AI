# FinSage AI Environment Configuration
# Copy this file to .env and fill in your values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017/
MONGODB_DB_NAME=finsage_db

# Security & Privacy
ANONYMIZATION_SALT=change_this_to_random_string
ENCRYPTION_KEY=change_this_to_secure_key

# RL Model Configuration
RL_MODEL_PATH=models/rl_budget_agent.pth

# Application Settings
ENV=development
DEBUG=true
LOG_LEVEL=info

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# API Rate Limiting (requests per minute)
RATE_LIMIT=60
